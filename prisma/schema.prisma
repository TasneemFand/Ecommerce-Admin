// prisma/schema.prisma
datasource db {
  provider  = "mongodb"
  url  	    = env("MONGO_URI")
}

generator client {
  provider = "prisma-client-js"
}


model User {
  id            String    @id @default(cuid()) @map("_id")
  name          String?
  email         String?   @unique
  password      String
}

